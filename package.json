{
	"name": "enuts",
	"description": "A cashu wallet for Android and IOS",
	"author": "Agron Kadriaj <m.echerbebo@gmail.com> (https://agron.dev)",
	"scripts": {
		"s:debug:full": "pnpm dlx cross-env@latest DEBUG=full pnpm s:c",
		"s:c:dev": "pnpm s:c -- --dev-client",
		"s:c": "pnpm start -- -c",
		"start": "expo start",
		"lint": "pnpm eslint . --ext .tsx,.ts --fix && pnpm tsc && pnpm fmtJson",
		"ts:check": "pnpm tsc",
		"dep:update": "pnpm up -L && npm i && npm audit fix || true && pnpm expo install --fix && pnpm lint",
		"dep:check": "pnpm dlx depcheck@latest",
		"fmtJson": "pnpm prettier *.json *.yaml .github/workflows/*.yml --write --use-tabs --tab-width 4",
		"expo:i:check": "pnpm expo install --check",
		"expo:i:fix": "pnpm expo install --fix",
		"expo:doc": "pnpm dlx expo-doctor@latest",
		"build:dev": "pnpm dlx eas-cli@latest build -e development -p android -m devWithDevClient --no-wait",
		"build:preview": "pnpm dlx eas-cli@latest build -e preview -p android -m previewNoDevClient --no-wait",
		"build:ci": "pnpm dlx eas-cli@latest build --non-interactive -e preview -p android -m ciPreviewBuild --no-wait",
		"eas-build-on-success": "pnpm bugsnag-eas-build-on-success",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"test": "pnpm jest -c config/jest.config.ts --passWithNoTests ",
		"test:coverage": "pnpm test -- --collectCoverage",
		"test:ts": "pnpm test --testPathIgnorePatterns=tsx$",
		"test:ts:coverage": "pnpm test:ts -- --collectCoverageFrom=src/**/*.ts",
		"test:tsx": "pnpm test --testPathIgnorePatterns=ts$",
		"test:tsx:coverage": "pnpm test:tsx -- --collectCoverageFrom=components/**/*.tsx",
		"test:dev": "pnpm test -- --detectOpenHandles",
		"cpd:ts": "pnpm dlx jscpd -c config/cpd.config.ts.json src config",
		"cpd:tsx": "pnpm dlx jscpd -c config/cpd.config.tsx.json src",
		"cpd": "pnpm cpd:ts && pnpm cpd:tsx",
		"deps:c": "pnpm dlx madge --circular --ts-config tsconfig.json --extensions ts,tsx ."
	},
	"dependencies": {
		"@babel/runtime": "^7.22.5",
